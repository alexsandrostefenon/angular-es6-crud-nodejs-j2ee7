{
  "manifest_version": 2,
  "name": "computer-vision",
  "version": "1.0.4",
  "description": "Demonstrates how to use permissions to load remote content",
  "homepage_url": "http://localhost:9080/ponfac-web-ocr",
  "offline_enabled": true,
  "permissions": ["idle", "tabs"],

  "start_url": ".",
  "display": "standalone",
  "developer": {
    "name": "Your name",
    "url": "http://example.com"
  },
  "type": "privileged",
  "installs_allowed_from": [
    "*"
  ]

}
